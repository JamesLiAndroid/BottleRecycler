<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/background">

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:gravity="center_horizontal"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="1235.0px"
            android:layout_height="0.0px"
            android:layout_weight="3.0"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0.0px"
                android:layout_height="fill_parent"
                android:layout_weight="3.0"
                android:orientation="horizontal" />

            <LinearLayout
                android:layout_width="0.0px"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/thank_page_for_paper_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="63.0px"
                    android:layout_marginTop="130.0px"
                    android:textColor="@color/white"
                    android:textSize="30.0sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/thank_page_for_paper_time_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="130.0px"
                    android:text="@string/dao_ji_shi"
                    android:textColor="@color/white"
                    android:textSize="@dimen/serial_number_size" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/thank_page_for_paper_layout"
            android:layout_width="fill_parent"
            android:layout_height="0.0px"
            android:layout_marginLeft="100.0px"
            android:layout_marginRight="104.0px"
            android:layout_weight="5.4"
            android:background="@drawable/green_bg"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0.0px"
                android:layout_height="fill_parent"
                android:layout_weight="3.0"
                android:gravity="center_vertical"
                android:orientation="horizontal" />

            <LinearLayout
                android:layout_width="0.0px"
                android:layout_height="468.0px"
                android:layout_margin="7.0px"
                android:layout_weight="5.0"
                android:gravity="center_horizontal"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="0.0px"
                    android:layout_weight="2.0"
                    android:orientation="horizontal" />

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="0.0px"
                    android:layout_marginBottom="25.0px"
                    android:layout_marginLeft="13.0px"
                    android:layout_marginRight="20.0px"
                    android:layout_weight="3.0"
                    android:background="@drawable/liebiao_bg"
                    android:orientation="vertical">

                    <TableLayout
                        android:id="@+id/PaperCount"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:stretchColumns="0,1,2">

                        <TableRow
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="0.5px">

                            <TextView
                                android:id="@+id/texture"
                                android:layout_width="fill_parent"
                                android:layout_height="52.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="6.0px"
                                android:gravity="center"
                                android:text="@string/texture"
                                android:textColor="@color/white"
                                android:textSize="25.0sp" />

                            <TextView
                                android:id="@+id/weight"
                                android:layout_width="fill_parent"
                                android:layout_height="52.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="6.0px"
                                android:gravity="center"
                                android:text="@string/weight"
                                android:textColor="@color/white"
                                android:textSize="25.0sp" />

                            <TextView
                                android:id="@+id/paper_amount"
                                android:layout_width="fill_parent"
                                android:layout_height="52.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="6.0px"
                                android:gravity="center"
                                android:text="@string/amount"
                                android:textColor="@color/white"
                                android:textSize="25.0sp" />
                        </TableRow>

                        <TableRow
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content">

                            <TextView
                                android:layout_width="fill_parent"
                                android:layout_height="46.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="4.0px"
                                android:gravity="center"
                                android:text="@string/recycle_paper"
                                android:textColor="@color/white"
                                android:textSize="20.0sp" />

                            <TextView
                                android:id="@+id/paper_count"
                                android:layout_width="fill_parent"
                                android:layout_height="46.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="4.0px"
                                android:gravity="center"
                                android:textColor="@color/white"
                                android:textSize="20.0sp" />

                            <TextView
                                android:id="@+id/paper_total_acount"
                                android:layout_width="fill_parent"
                                android:layout_height="46.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="4.0px"
                                android:gravity="center"
                                android:textColor="@color/white"
                                android:textSize="20.0sp" />
                        </TableRow>
                    </TableLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0.0px"
            android:layout_weight="1.8"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="905.0px"
                android:layout_height="fill_parent"
                android:layout_marginLeft="110.0px"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/thank_page_for_paper_remind_text"
                    android:layout_width="fill_parent"
                    android:layout_height="136.0px"
                    android:layout_marginTop="8.0px"
                    android:background="@drawable/black_bg"
                    android:gravity="center"
                    android:padding="10.0px"
                    android:text="@string/thank"
                    android:textSize="@dimen/remind_text_size" />
            </LinearLayout>

            <Button
                android:id="@+id/thank_page_for_paper_end"
                android:layout_width="156.0px"
                android:layout_height="156.0px"
                android:background="@drawable/jieshu_btn"
                android:paddingTop="72.0px"
                android:text="@string/end"
                android:textColor="@color/white"
                android:textSize="@dimen/linear_right_button_textSize"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0.0px"
            android:layout_weight="0.8"
            android:orientation="horizontal" />
    </LinearLayout>
</AbsoluteLayout>