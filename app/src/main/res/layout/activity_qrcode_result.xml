<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/background">

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:gravity="center_horizontal"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="1235.0px"
            android:layout_height="0.0px"
            android:layout_weight="3.0"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0.0px"
                android:layout_height="fill_parent"
                android:layout_weight="3.0"
                android:orientation="horizontal" />

            <LinearLayout
                android:layout_width="0.0px"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/QRcode_rebate_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="50.0px"
                    android:layout_marginTop="100.0px"
                    android:textColor="@color/white"
                    android:textSize="30.0sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/QRcode_rebate_time_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="100.0px"
                    android:text="@string/dao_ji_shi"
                    android:textColor="@color/white"
                    android:textSize="@dimen/serial_number_size" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/QRcode_rebate_layout"
            android:layout_width="fill_parent"
            android:layout_height="0.0px"
            android:layout_marginLeft="80.0px"
            android:layout_marginRight="80.0px"
            android:layout_weight="5.4"
            android:background="@drawable/green_bg"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0.0px"
                android:layout_height="fill_parent"
                android:layout_marginBottom="13.0px"
                android:layout_marginLeft="10.0px"
                android:layout_marginTop="5.0px"
                android:layout_weight="3.0"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="75.0px"
                    android:layout_height="75.0px"
                    android:background="@drawable/lvniukou_icon" />

                <TextView
                    style="@style/blue_bg_shadow"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10.0px"
                    android:text="@string/Gqrcode_rebate"
                    android:textColor="@color/white"
                    android:textSize="@dimen/onecard_textsize" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="0.0px"
                android:layout_height="468.0px"
                android:layout_margin="5.0px"
                android:layout_weight="5.0"
                android:gravity="center_horizontal"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="0.0px"
                    android:layout_weight="2.0"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/society_QRcode_info"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:orientation="vertical">

                        <LinearLayout
                            android:id="@+id/QRcode_rebate_info"
                            android:layout_width="663.0px"
                            android:layout_height="0.0px"
                            android:layout_weight="1.0"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <TextView
                                style="@style/blue_bg_shadow"
                                android:layout_width="313.0px"
                                android:layout_height="wrap_content"
                                android:gravity="center|right"
                                android:text="@string/recharge_this"
                                android:textColor="@color/white"
                                android:textSize="20.0sp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/QRcode_rebate_this"
                                style="@style/blue_bg_shadow"
                                android:layout_width="125.0px"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="10.0px"
                                android:gravity="center"
                                android:textColor="@color/white"
                                android:textSize="30.0sp" />

                            <TextView
                                style="@style/blue_bg_shadow"
                                android:layout_width="125.0px"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="10.0px"
                                android:gravity="center_vertical"
                                android:text="@string/yuan"
                                android:textColor="@color/white"
                                android:textSize="20.0sp"
                                android:textStyle="bold" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/QRcode_balance_info"
                            android:layout_width="663.0px"
                            android:layout_height="0.0px"
                            android:layout_weight="1.0"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <TextView
                                style="@style/blue_bg_shadow"
                                android:layout_width="313.0px"
                                android:layout_height="wrap_content"
                                android:gravity="center|right"
                                android:text="@string/rebate_total"
                                android:textColor="@color/white"
                                android:textSize="20.0sp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/QRcode_INCOM_balance"
                                style="@style/blue_bg_shadow"
                                android:layout_width="125.0px"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="10.0px"
                                android:gravity="center"
                                android:textSize="30.0sp" />

                            <TextView
                                style="@style/blue_bg_shadow"
                                android:layout_width="125.0px"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="10.0px"
                                android:gravity="center_vertical"
                                android:text="@string/yuan"
                                android:textColor="@color/white"
                                android:textSize="20.0sp"
                                android:textStyle="bold" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/all_jifen_info"
                            android:layout_width="625.0px"
                            android:layout_height="0.0px"
                            android:layout_weight="1.0"
                            android:orientation="horizontal"
                            android:visibility="gone">

                            <TextView
                                android:id="@+id/integrationText"
                                style="@style/blue_bg_shadow"
                                android:layout_width="313.0px"
                                android:layout_height="wrap_content"
                                android:gravity="center|right"
                                android:text="@string/integration"
                                android:textColor="@color/white"
                                android:textSize="20.0sp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/integration"
                                style="@style/blue_bg_shadow"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20.0px"
                                android:gravity="center|left"
                                android:textColor="@color/white"
                                android:textSize="30.0sp" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/school_QRcode_info"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:gravity="center_vertical"
                        android:orientation="vertical"
                        android:visibility="gone">

                        <RelativeLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:padding="10.0px">

                            <TextView
                                android:id="@+id/school_QRcode_qrcode_result"
                                style="@style/blue_bg_shadow"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="center_vertical"
                                android:text="@string/qrcode_result"
                                android:textColor="@color/white"
                                android:textSize="20.0sp"
                                android:textStyle="bold"
                                android:visibility="gone" />

                            <TextView
                                android:id="@+id/school_QRcode_info_text"
                                style="@style/blue_bg_shadow"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_toRightOf="@+id/school_QRcode_qrcode_result"
                                android:gravity="center_vertical"
                                android:text="@string/qeCode_unuse"
                                android:textColor="@color/white"
                                android:textSize="20.0sp"
                                android:textStyle="bold" />
                        </RelativeLayout>

                        <LinearLayout
                            android:id="@+id/all_jifen_info2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:orientation="horizontal"
                            android:visibility="gone">

                            <TextView
                                android:id="@+id/integrationText2"
                                style="@style/blue_bg_shadow"
                                android:layout_width="188.0px"
                                android:layout_height="wrap_content"
                                android:gravity="center|right"
                                android:text="@string/integration"
                                android:textColor="@color/white"
                                android:textSize="20.0sp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/integration2"
                                style="@style/blue_bg_shadow"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="20.0px"
                                android:gravity="center_vertical"
                                android:textColor="@color/white"
                                android:textSize="25.0sp"
                                android:textStyle="bold" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="0.0px"
                    android:layout_marginBottom="20.0px"
                    android:layout_marginLeft="10.0px"
                    android:layout_marginRight="20.0px"
                    android:layout_weight="3.0"
                    android:background="@drawable/liebiao_bg"
                    android:orientation="vertical">

                    <TableLayout
                        android:id="@+id/BottleCount"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:stretchColumns="0,1,2">

                        <TableRow
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="0.5px">

                            <TextView
                                android:id="@+id/catacity"
                                android:layout_width="fill_parent"
                                android:layout_height="52.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="5.0px"
                                android:gravity="center"
                                android:text="@string/catacity"
                                android:textColor="@color/white"
                                android:textSize="17.0sp" />

                            <TextView
                                android:id="@+id/quantity"
                                android:layout_width="fill_parent"
                                android:layout_height="52.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="5.0px"
                                android:gravity="center"
                                android:text="@string/quantity"
                                android:textColor="@color/white"
                                android:textSize="17.0sp" />

                            <TextView
                                android:id="@+id/donationquantity"
                                android:layout_width="fill_parent"
                                android:layout_height="52.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="5.0px"
                                android:gravity="center"
                                android:text="@string/donationquantity"
                                android:textColor="@color/white"
                                android:textSize="17.0sp" />

                            <TextView
                                android:id="@+id/amount"
                                android:layout_width="fill_parent"
                                android:layout_height="52.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="5.0px"
                                android:gravity="center"
                                android:text="@string/amount"
                                android:textColor="@color/white"
                                android:textSize="17.0sp" />
                        </TableRow>

                        <TableRow
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content">

                            <TextView
                                android:layout_width="fill_parent"
                                android:layout_height="46.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="3.0px"
                                android:gravity="center"
                                android:text="@string/milliliter"
                                android:textColor="@color/white"
                                android:textSize="17.0sp" />

                            <TextView
                                android:id="@+id/onetotal_number"
                                android:layout_width="fill_parent"
                                android:layout_height="46.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="3.0px"
                                android:gravity="center"
                                android:textColor="@color/white"
                                android:textSize="17.0sp" />

                            <TextView
                                android:id="@+id/onetotaldonation_number"
                                android:layout_width="fill_parent"
                                android:layout_height="46.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="3.0px"
                                android:gravity="center"
                                android:textColor="@color/white"
                                android:textSize="17.0sp" />

                            <TextView
                                android:id="@+id/onetotal_acount"
                                android:layout_width="fill_parent"
                                android:layout_height="46.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="3.0px"
                                android:gravity="center"
                                android:textColor="@color/white"
                                android:textSize="17.0sp" />
                        </TableRow>

                        <TableRow
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content">

                            <TextView
                                android:layout_width="fill_parent"
                                android:layout_height="46.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="3.0px"
                                android:gravity="center"
                                android:text="500ml-1.2L"
                                android:textColor="@color/white"
                                android:textSize="17.0sp" />

                            <TextView
                                android:id="@+id/twototal_number"
                                android:layout_width="fill_parent"
                                android:layout_height="46.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="3.0px"
                                android:gravity="center"
                                android:textColor="@color/white"
                                android:textSize="17.0sp" />

                            <TextView
                                android:id="@+id/twototaldonation_number"
                                android:layout_width="fill_parent"
                                android:layout_height="46.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="3.0px"
                                android:gravity="center"
                                android:textColor="@color/white"
                                android:textSize="17.0sp" />

                            <TextView
                                android:id="@+id/twototal_amount"
                                android:layout_width="fill_parent"
                                android:layout_height="46.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="3.0px"
                                android:gravity="center"
                                android:textColor="@color/white"
                                android:textSize="17.0sp" />
                        </TableRow>

                        <TableRow
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content">

                            <TextView
                                android:layout_width="fill_parent"
                                android:layout_height="46.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="3.0px"
                                android:gravity="center"
                                android:text="1.2L-2L"
                                android:textColor="@color/white"
                                android:textSize="17.0sp" />

                            <TextView
                                android:id="@+id/threetotal_number"
                                android:layout_width="fill_parent"
                                android:layout_height="46.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="3.0px"
                                android:gravity="center"
                                android:textColor="@color/white"
                                android:textSize="17.0sp" />

                            <TextView
                                android:id="@+id/threetotaldonation_number"
                                android:layout_width="fill_parent"
                                android:layout_height="46.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="3.0px"
                                android:gravity="center"
                                android:textColor="@color/white"
                                android:textSize="17.0sp" />

                            <TextView
                                android:id="@+id/threetotal_amount"
                                android:layout_width="fill_parent"
                                android:layout_height="46.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="3.0px"
                                android:gravity="center"
                                android:textColor="@color/white"
                                android:textSize="17.0sp" />
                        </TableRow>

                        <TableRow
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="0.5px">

                            <TextView
                                android:id="@+id/amount1"
                                android:layout_width="fill_parent"
                                android:layout_height="46.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="3.0px"
                                android:gravity="center"
                                android:text="@string/amount1"
                                android:textColor="@color/white"
                                android:textSize="17.0sp" />

                            <TextView
                                android:id="@+id/total_number"
                                android:layout_width="fill_parent"
                                android:layout_height="46.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="3.0px"
                                android:gravity="center"
                                android:textColor="@color/white"
                                android:textSize="17.0sp" />

                            <TextView
                                android:id="@+id/totaldonation_number"
                                android:layout_width="fill_parent"
                                android:layout_height="46.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="3.0px"
                                android:gravity="center"
                                android:textColor="@color/white"
                                android:textSize="17.0sp" />

                            <TextView
                                android:id="@+id/total_amount"
                                android:layout_width="fill_parent"
                                android:layout_height="46.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="3.0px"
                                android:gravity="center"
                                android:textColor="@color/white"
                                android:textSize="17.0sp" />
                        </TableRow>
                    </TableLayout>

                    <TableLayout
                        android:id="@+id/PaperCount"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:stretchColumns="0,1,2"
                        android:visibility="gone">

                        <TableRow
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="0.5px">

                            <TextView
                                android:id="@+id/texture"
                                android:layout_width="fill_parent"
                                android:layout_height="52.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="5.0px"
                                android:gravity="center"
                                android:text="@string/texture"
                                android:textColor="@color/white"
                                android:textSize="25.0sp" />

                            <TextView
                                android:id="@+id/weight"
                                android:layout_width="fill_parent"
                                android:layout_height="52.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="5.0px"
                                android:gravity="center"
                                android:text="@string/weight"
                                android:textColor="@color/white"
                                android:textSize="25.0sp" />

                            <TextView
                                android:id="@+id/paper_amount"
                                android:layout_width="fill_parent"
                                android:layout_height="52.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="5.0px"
                                android:gravity="center"
                                android:text="@string/amount"
                                android:textColor="@color/white"
                                android:textSize="25.0sp" />
                        </TableRow>

                        <TableRow
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content">

                            <TextView
                                android:layout_width="fill_parent"
                                android:layout_height="46.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="3.0px"
                                android:gravity="center"
                                android:text="@string/recycle_paper"
                                android:textColor="@color/white"
                                android:textSize="20.0sp" />

                            <TextView
                                android:id="@+id/paper_count"
                                android:layout_width="fill_parent"
                                android:layout_height="46.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="3.0px"
                                android:gravity="center"
                                android:textColor="@color/white"
                                android:textSize="20.0sp" />

                            <TextView
                                android:id="@+id/paper_total_acount"
                                android:layout_width="fill_parent"
                                android:layout_height="46.0px"
                                android:layout_gravity="center_vertical"
                                android:layout_margin="3.0px"
                                android:gravity="center"
                                android:textColor="@color/white"
                                android:textSize="20.0sp" />
                        </TableRow>
                    </TableLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0.0px"
            android:layout_weight="1.8"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="953.0px"
                android:layout_height="fill_parent"
                android:layout_marginLeft="90.0px"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/QRcode_rebate_remind_text"
                    android:layout_width="fill_parent"
                    android:layout_height="133.0px"
                    android:layout_marginTop="8.0px"
                    android:background="@drawable/black_bg"
                    android:gravity="center"
                    android:padding="10.0px"
                    android:textSize="@dimen/remind_text_size" />
            </LinearLayout>

            <Button
                android:id="@+id/QRcode_rebate_end"
                android:layout_width="156.0px"
                android:layout_height="156.0px"
                android:background="@drawable/queding_btn"
                android:paddingTop="72.0px"
                android:text="@string/confirm"
                android:textColor="@color/white"
                android:textSize="@dimen/linear_right_button_textSize"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0.0px"
            android:layout_weight="0.8"
            android:orientation="horizontal" />
    </LinearLayout>
</AbsoluteLayout>