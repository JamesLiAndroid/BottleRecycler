<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/background">

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:gravity="center_horizontal"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="1235.0px"
            android:layout_height="0.0px"
            android:layout_weight="3.0"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0.0px"
                android:layout_height="fill_parent"
                android:layout_weight="3.0"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/software_version_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="263.0px"
                    android:layout_marginTop="106.0px"
                    android:text="@string/softwareVersion"
                    android:textSize="@dimen/serial_number_size" />

                <TextView
                    android:id="@+id/software_version"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="106.0px"
                    android:textSize="@dimen/serial_number_size" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="0.0px"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/channel_advance__time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="50.0px"
                    android:layout_marginTop="100.0px"
                    android:textColor="@color/white"
                    android:textSize="30.0sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/channel_advance__time_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="100.0px"
                    android:text="@string/dao_ji_shi"
                    android:textColor="@color/white"
                    android:textSize="@dimen/serial_number_size" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/channel_advance__layout"
            android:layout_width="fill_parent"
            android:layout_height="0.0px"
            android:layout_marginBottom="14.0px"
            android:layout_marginLeft="80.0px"
            android:layout_marginRight="80.0px"
            android:layout_marginTop="5.0px"
            android:layout_weight="5.4"
            android:background="@drawable/h_bg"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0.0px"
                android:layout_height="fill_parent"
                android:layout_weight="3.0"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/channel_advance_linnearlayout"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginBottom="5.0px"
                    android:layout_marginLeft="5.0px"
                    android:layout_marginTop="5.0px"
                    android:gravity="center_horizontal"
                    android:orientation="vertical" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/channel_advance__button_layout"
                android:layout_width="0.0px"
                android:layout_height="fill_parent"
                android:layout_marginBottom="5.0px"
                android:layout_marginRight="10.0px"
                android:layout_marginTop="5.0px"
                android:layout_weight="2.0"
                android:gravity="center_horizontal"
                android:orientation="vertical">

                <Button
                    android:id="@+id/systemSettingsButton"
                    style="@style/blue_bg_shadow"
                    android:layout_width="fill_parent"
                    android:layout_height="85.0px"
                    android:layout_marginTop="5.0px"
                    android:background="@drawable/tongdaoweihu_gaoji_btn_bg"
                    android:text="@string/systemSettings"
                    android:textColor="@color/button_textColor"
                    android:textSize="@dimen/linear_left_editsize" />

                <Button
                    android:id="@+id/showNetWorkTestButton"
                    style="@style/blue_bg_shadow"
                    android:layout_width="fill_parent"
                    android:layout_height="85.0px"
                    android:layout_marginTop="5.0px"
                    android:background="@drawable/tongdaoweihu_gaoji_btn_bg"
                    android:text="@string/netWorkTestBtn"
                    android:textColor="@color/button_textColor"
                    android:textSize="@dimen/linear_left_editsize" />

                <Button
                    android:id="@+id/operDataButton"
                    style="@style/blue_bg_shadow"
                    android:layout_width="fill_parent"
                    android:layout_height="85.0px"
                    android:layout_marginTop="5.0px"
                    android:background="@drawable/tongdaoweihu_gaoji_btn_bg"
                    android:text="@string/operDataBtn"
                    android:textColor="@color/button_textColor"
                    android:textSize="@dimen/linear_left_editsize" />

                <Button
                    android:id="@+id/netCfgButton"
                    style="@style/blue_bg_shadow"
                    android:layout_width="fill_parent"
                    android:layout_height="85.0px"
                    android:layout_marginTop="5.0px"
                    android:background="@drawable/tongdaoweihu_gaoji_btn_bg"
                    android:text="@string/netCfgBtn"
                    android:textColor="@color/button_textColor"
                    android:textSize="@dimen/linear_left_editsize" />

                <Button
                    android:id="@+id/serCfgButton"
                    style="@style/blue_bg_shadow"
                    android:layout_width="fill_parent"
                    android:layout_height="85.0px"
                    android:layout_marginBottom="5.0px"
                    android:layout_marginTop="5.0px"
                    android:background="@drawable/tongdaoweihu_gaoji_btn_bg"
                    android:text="@string/serCfgBtn"
                    android:textColor="@color/button_textColor"
                    android:textSize="@dimen/linear_left_editsize" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0.0px"
            android:layout_marginLeft="75.0px"
            android:layout_marginRight="75.0px"
            android:layout_weight="1.8"
            android:orientation="horizontal">

            <Button
                android:id="@+id/channel_advance__return_btn"
                android:layout_width="156.0px"
                android:layout_height="156.0px"
                android:background="@drawable/fanhui_btn"
                android:paddingTop="65.0px"
                android:text="@string/backBtn"
                android:textColor="@color/white"
                android:textSize="@dimen/linear_right_button_textSize"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="0.0px"
                android:layout_height="fill_parent"
                android:layout_weight="1.0"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/channel_advance_remind_text"
                    android:layout_width="fill_parent"
                    android:layout_height="132.0px"
                    android:layout_marginTop="7.0px"
                    android:background="@drawable/black_bg"
                    android:gravity="center"
                    android:padding="10.0px"
                    android:text="@string/help_info"
                    android:textSize="@dimen/remind_text_size" />
            </LinearLayout>

            <Button
                android:id="@+id/channel_advance_exit_btn"
                android:layout_width="156.0px"
                android:layout_height="156.0px"
                android:background="@drawable/rvm_exit_btn"
                android:paddingTop="65.0px"
                android:text="@string/exitBtn"
                android:textColor="@color/white"
                android:textSize="@dimen/linear_right_button_textSize"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="0.0px"
            android:layout_weight="0.8"
            android:orientation="horizontal" />
    </LinearLayout>
</AbsoluteLayout>